<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="adminChangingPane" prefHeight="650.0" prefWidth="1041.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.projectggmountain.controller.ReportFormController">
   <children>
      <ImageView fitHeight="650.0" fitWidth="1041.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="650.0" fitWidth="1041.0">
         <image>
            <Image url="@../assets/dashBord.png" />
         </image>
      </ImageView>
      <Button fx:id="employeeBtn" layoutX="90.0" layoutY="120.0" mnemonicParsing="false" onAction="#onActionemployeeBtn" prefHeight="39.0" prefWidth="144.0" text="Employee">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <Button fx:id="supplyLoadDetailBtn" layoutX="90.0" layoutY="190.0" mnemonicParsing="false" onAction="#onActionsupplyLoadDetailBtn" prefHeight="39.0" prefWidth="144.0" text="Supply Load Detail">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>

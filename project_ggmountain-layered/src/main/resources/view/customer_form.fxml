         <columns>
            <TableColumn fx:id="custColId" prefWidth="68.6666259765625" text="ID" />
            <TableColumn fx:id="custColName" prefWidth="110.66668701171875" text="Name" />
            <TableColumn fx:id="custColAddress" prefWidth="151.33334350585938" text="Address" />
            <TableColumn fx:id="custColEmail" prefWidth="168.66668701171875" text="E Mail" />
            <TableColumn fx:id="custColContact" prefWidth="127.33331298828125" text="Contact" />
         </columns>
      </TableView>
      <Label layoutX="56.0" layoutY="111.0" text="Customer ID:" textFill="WHITE" />
      <Label layoutX="794.0" layoutY="111.0" text="Address:" textFill="WHITE" />
      <Label layoutX="428.0" layoutY="111.0" text="Customer Name:" textFill="WHITE" />
      <Label layoutX="56.0" layoutY="234.0" text="E Mail:" textFill="WHITE" />
      <Label layoutX="428.0" layoutY="234.0" text="Contact:" textFill="WHITE" />
      <Button fx:id="saveBtn" layoutX="738.0" layoutY="302.0" mnemonicParsing="false" onAction="#clickOnActionSave" prefHeight="25.0" prefWidth="72.0" styleClass="employeeFeild" stylesheets="/styles/style.css" text="Save">
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="updateBtn" layoutX="826.0" layoutY="302.0" mnemonicParsing="false" onAction="#clickOnActionUpdate" prefHeight="25.0" prefWidth="72.0" styleClass="employeeFeild" stylesheets="/styles/style.css" text="Update">
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="deleteBtn" layoutX="907.0" layoutY="302.0" mnemonicParsing="false" onAction="#clickOnActionDelete" prefHeight="25.0" prefWidth="72.0" styleClass="employeeFeild" stylesheets="/styles/style.css" text="Delete">
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <TextField fx:id="txtSearch" layoutX="795.0" layoutY="47.0" onAction="#txtSearchOnAction" onKeyTyped="#txtSearchKeyTyped" opacity="0.83" prefHeight="25.0" prefWidth="205.0" promptText="Search by ID" styleClass="employeeFeild" stylesheets="/styles/style.css" />
      <ImageView fx:id="searchIcon" fitHeight="25.0" fitWidth="18.0" layoutX="970.0" layoutY="51.0" onMousePressed="#searchIconOnMousePressed" opacity="0.37" pickOnBounds="true" preserveRatio="true">

<AnchorPane fx:id="adminChangingPane" prefHeight="650.0" prefWidth="1041.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.projectggmountain.controller.CustomerFormController">
   <children>
      <ImageView fitHeight="650.0" fitWidth="1041.0" layoutX="1.0" layoutY="1.0">
         <image>
            <Image url="@../assets/dashBord.png" />
         </image>
      </ImageView>
      <Label layoutX="401.0" layoutY="40.0" text="Customer Details" textFill="#e61a5a">
         <font>
            <Font name="Times New Roman Bold Italic" size="36.0" />
         </font>
      </Label>
      <TextField fx:id="custId" layoutX="55.0" layoutY="130.0" onMousePressed="#custIdOnMousePressed" prefHeight="25.0" prefWidth="205.0" styleClass="employeeFeild" stylesheets="/styles/style.css" />
      <TextField fx:id="custName" layoutX="427.0" layoutY="130.0" prefHeight="25.0" prefWidth="205.0" styleClass="employeeFeild" stylesheets="/styles/style.css" />
      <TextField fx:id="custAddress" layoutX="795.0" layoutY="130.0" prefHeight="25.0" prefWidth="205.0" styleClass="employeeFeild" stylesheets="/styles/style.css" />
      <TextField fx:id="custContact" layoutX="428.0" layoutY="256.0" onKeyPressed="#custContactOnMousePressed" prefHeight="25.0" prefWidth="205.0" styleClass="employeeFeild" stylesheets="/styles/style.css" />
      <TextField fx:id="custEmail" layoutX="55.0" layoutY="256.0" onKeyPressed="#custEmailOnMousePressed" prefHeight="25.0" prefWidth="205.0" styleClass="employeeFeild" stylesheets="/styles/style.css" />
